{
  "$schema": "https://platformatic.dev/schemas/v1.23.0/service",
  "service": {
    "openapi": true
  },
  "watch": true,
  "plugins": {
    "paths": [
      {
        "path": "./plugins",
        "encapsulate": false
      },
      "./routes"
    ],
    "typescript": false
  },
  "server": {
    "hostname": "{PLT_SERVER_HOSTNAME}",
    "port": 3042,
    "logger": {
      "level": "info"
    }
  }
}